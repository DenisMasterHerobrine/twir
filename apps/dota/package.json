{
  "name": "@tsuwari/dota",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "dev": "reflex -s -r '\\.ts$' -- sh -c 'cross-env TS_NODE_TRANSPILE_ONLY=true node --loader ts-node/esm --enable-source-maps --trace-warnings --inspect=0.0.0.0:9231 --nolazy src/index.ts'",
    "start": "node --enable-source-maps dist/index.js"
  },
  "devDependencies": {
    "@types/steam-user": "4.26.0",
    "@types/steamid": "2.0.1"
  },
  "dependencies": {
    "@tsuwari/config": "workspace:*",
    "@tsuwari/grpc": "workspace:*",
    "@tsuwari/shared": "workspace:*",
    "@tsuwari/typeorm": "workspace:*",
    "@types/lodash": "^4.14.191",
    "ioredis": "5.2.4",
    "lodash": "4.17.21",
    "nice-grpc": "^2.0.1",
    "protobufjs": "7.1.2",
    "reflect-metadata": "0.1.13",
    "steam-user": "4.26.0",
    "steamid": "2.0.0",
    "tslib": "2.4.1"
  }
}