{
  "name": "@tsuwari/integrations",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "dev": "reflex -s -g 'src/**/*.ts' -- sh -c 'cross-env TS_NODE_TRANSPILE_ONLY=true node --loader ts-node/esm --enable-source-maps --trace-warnings --inspect=0.0.0.0:9232 --nolazy src/index.ts'",
    "start": "node --enable-source-maps dist/index.js"
  },
  "type": "module",
  "dependencies": {
    "@tsuwari/config": "workspace:*",
    "@tsuwari/grpc": "workspace:*",
    "@tsuwari/shared": "workspace:*",
    "@tsuwari/typeorm": "workspace:*",
    "@twurple/api": "^5.2.7",
    "@twurple/auth": "^5.2.7",
    "centrifuge": "2.8.5",
    "nice-grpc": "^2.0.1",
    "socket.io-client": "2.3.1",
    "ws": "^8.11.0",
    "xmlhttprequest": "^1.8.0"
  },
  "devDependencies": {
    "@types/socket.io-client": "1.4.36",
    "@types/ws": "^8.5.3"
  }
}