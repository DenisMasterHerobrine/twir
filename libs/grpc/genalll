protoc --go_out=./go --go_opt=paths=source_relative \
    --go-grpc_out=./go --go-grpc_opt=paths=source_relative \
    ./src/*.proto

MODULE=streamstatus protoc \
--proto_path=. \
--go_out=./go \
--go_opt=module=${MODULE} \
--go-grpc_out=./go \
--go-grpc_opt=module=${MODULE} \
./src/streamstatus.proto

protoc \
--proto_path=. \
--go_out=./go \
--go-grpc_out=./go \
./src/streamstatus.proto